<h1>{{ header }}</h1>
<a href="/add">Add Provider</a>

<table>
  <tr>
    <th>Name</th>
    <th>Specialty</th>
    <th>Phone</th>
    <th>email</th>
    <th>Password</th>
    <th></th>
  </tr>
  {{#each providers}}
    <tr><td>{{this.name}}</td>
    <td>{{this.email}}</td>
    <td>{{this.phone}}</td>
    <td>{{this.password}}</td>
    <td><a href="/update/{{this._id}}">Edit</a></td>
    <a href="/delete/{{this._id}}"> Delete </a></td></tr>
  {{/each}}
</table>
